<!--logs.wxml-->
<view class="my-swiper">
      <swiper indicator-dots="{{indicatorDots}}"
    autoplay interval="3000" duration="1000">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" height="150"/>
      </swiper-item>
    </block>
  </swiper>
  </view>

